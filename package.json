    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "moment": "^2.29.3",
    "nodemon": "^2.0.15",
    "sequelize": "^6.19.0",
    "sqlite3": "^5.0.4"
  },
  "devDependencies": {
